<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:id="@+id/linearLayout_Top"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:orientation="horizontal">
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout_Top"
        android:id="@+id/menuGroup1_topLine"
        android:background="@color/graysboro"
        android:layout_height="1dp"/>

    <LinearLayout
        style="@style/Setting_LinearLayout"
        android:id="@+id/linearLayout_Resign"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menuGroup1_topLine"
        android:layout_marginTop="@dimen/setting_menu_margin_top">
        <TextView
            style="@style/Setting_TextView"
            android:text="회원탈퇴"/>
    </LinearLayout>

    <LinearLayout
        style="@style/Setting_LinearLayout"
        android:id="@+id/linearLayout_Logout"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout_Resign"
        android:layout_marginTop="@dimen/setting_menu_margin_top">>
        <TextView
            style="@style/Setting_TextView"
            android:text="로그아웃"/>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>